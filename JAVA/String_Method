package com.company;

import javax.sound.midi.Soundbank;

public class String_Methods {
    public static void main(String[] args) {
        String name="Shrinath Asati";
        System.out.println(name.length());
        //name=name.toLowerCase();
        //System.out.println(name);
        System.out.println(name.toLowerCase());
        System.out.println(name.toUpperCase());
        String s="  Raghunath Asati";
        System.out.println(name.trim());
        System.out.println(name.substring(0,14));
        System.out.println(name.substring(0,14));
        System.out.println(name.replace('s','t'));
        System.out.println(name.replace("Shri","sanu"));
        System.out.println(name.startsWith("Shri"));
        System.out.println(name.endsWith(" Asati"));
        System.out.println(name.charAt(3));
        System.out.println(name.indexOf("Shri"));
        System.out.println(name.indexOf("Shri",4));
        System.out.println(name.lastIndexOf("rina",12));
        System.out.println(name.equals("Shrinath Asati"));
        System.out.println(name.equalsIgnoreCase("ShriNath Asati"));
    }
}
